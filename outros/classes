// Classe SolicitacaoVendedor
class SolicitacaoVendedor {
    private $id;
    private $user_id;
    private $nome_loja;
    private $cnpj;
    private $email;
    private $telefone;
    private $cep;
    private $estado;
    private $cidade;
    private $bairro;
    private $rua;
    private $numero;
    private $categoria;
    private $descricao_loja;
    private $data_solicitacao;
    private $status;
    private $motivo_rejeicao;

    public function setId($id) {
        $this->id = $id;
    }
    public function getId() {
        return $this->id;
    }
    public function setUserId($user_id) {
        $this->user_id = $user_id;
    }
    public function getUserId() {
        return $this->user_id;
    }
    public function setNomeLoja($nome_loja) {
        $this->nome_loja = $nome_loja;
